{
  "name": "countries-challenge",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Jonathan Gac <jonathan.gac@icloud.com>",
  "license": "MIT",
  "scripts": {
    "develop": "concurrently -n next,graphql-codegen 'next dev' 'npm:generate -- --watch'",
    "prebuild": "yarn run generate",
    "build": "next build",
    "prestart": "yarn run generate",
    "start": "next start",
    "generate": "graphql-codegen",
    "storybook": "start-storybook -p 6006 --ci",
    "lint": "eslint --fix --ext .js,jsx,ts,tsx .",
    "release": "standard-version"
  },
  "dependencies": {
    "@apollo/client": "^3.3.9",
    "@nexus/schema": "^0.20.1",
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/postcss7-compat": "^2.0.3",
    "apollo-datasource-rest": "^0.10.0",
    "apollo-errors": "^1.9.0",
    "apollo-resolvers": "^1.4.2",
    "apollo-server-micro": "^2.20.0",
    "autoprefixer": "^9",
    "clsx": "^1.1.1",
    "graphql": "^15.5.0",
    "next": "^10.0.6",
    "postcss": "^7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@graphql-codegen/cli": "^1.20.1",
    "@graphql-codegen/typescript": "^1.21.0",
    "@graphql-codegen/typescript-operations": "^1.17.14",
    "@graphql-codegen/typescript-react-apollo": "^2.2.1",
    "@storybook/addon-actions": "^6.1.17",
    "@storybook/addon-essentials": "^6.1.17",
    "@storybook/addon-links": "^6.1.17",
    "@storybook/react": "^6.1.17",
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "babel-plugin-inline-react-svg": "^1.1.2",
    "concurrently": "^5.3.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "husky": "^4.2.1",
    "storybook-css-modules-preset": "^1.0.6",
    "typescript": "^4.1.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
